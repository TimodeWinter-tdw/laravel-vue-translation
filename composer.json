{
    "name": "timodw/laravel-vue-translation",
    "description": "A package to have Laravel translation feature in VueJs",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Timo de Winter",
            "email": "info@timodw.nl"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "elementaryframework/fire-fs": "1.0.0"
    },
    "autoload": {
        "psr-4": {
            "Timodw\\Translation\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Timodw\\Translation\\TranslationServiceProvider"
            ]
        }
    }
}
